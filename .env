# Database
DATABASE_URL=postgresql://youtube_user:youtube_pass@localhost:5432/youtube_downloads

# Redis
REDIS_URL=redis://youtube-download-redis:6379
CELERY_BROKER_URL=redis://youtube-download-redis:6379/0
CELERY_RESULT_BACKEND=redis://youtube-download-redis:6379/0

# API
API_V1_STR=/api/v1
PROJECT_NAME=YouTube Download API
DEBUG=true
SECRET_KEY=your-super-secret-key-for-jwt-tokens-2024

# YouTube Download
VIDEOS_DIR=videos
MAX_CONCURRENT_DOWNLOADS=1
TEMP_FILE_EXPIRATION=3600  # 1 hour in seconds

# Google Drive
UPLOAD_TO_DRIVE=false
GOOGLE_DRIVE_FOLDER_ID=
GOOGLE_CREDENTIALS_FILE=credentials.json

# Security
RATE_LIMIT_PER_MINUTE=60
RATE_LIMIT_PER_HOUR=1000

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json 
